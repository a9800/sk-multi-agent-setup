# Azure Configuration
AZURE_SUBSCRIPTION_ID=your-subscription-id
AZURE_TENANT_ID=your-tenant-id
AZURE_RESOURCE_GROUP=your-resource-group

# Azure OpenAI Configuration
AZURE_OPENAI_ENDPOINT=https://foundryuaenorth.cognitiveservices.azure.com/
AZURE_OPENAI_API_KEY=your-azure-openai-api-key

# Azure AI Project Configuration
AZURE_EXISTING_AIPROJECT_ENDPOINT=https://your-ai-project-endpoint.cognitiveservices.azure.com
AZURE_EXISTING_AIPROJECT_RESOURCE_ID=/subscriptions/{subscription-id}/resourceGroups/{resource-group}/providers/Microsoft.CognitiveServices/accounts/{resource-name}/projects/{project-name}

# Azure AI Agent Configuration
AZURE_EXISTING_AGENT_ID=your-primary-agent-id
AZURE_AI_AGENT_NAME=your-agent-name

# Azure AI Agent Endpoint for SK Orchestration
# This should be the same as AZURE_EXISTING_AIPROJECT_ENDPOINT for most cases
AZURE_AI_AGENT_ENDPOINT=https://your-ai-project-endpoint.cognitiveservices.azure.com

# Web App Authentication (Optional)
WEB_APP_USERNAME=your-username
WEB_APP_PASSWORD=your-password

# Logging Configuration
APP_LOG_FILE=app.log

# Tracing Configuration (Optional)
ENABLE_AZURE_MONITOR_TRACING=false

# Production Configuration
RUNNING_IN_PRODUCTION=false