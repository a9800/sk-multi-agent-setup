# Azure Configuration
AZURE_SUBSCRIPTION_ID=your-subscription-id
AZURE_TENANT_ID=your-tenant-id
AZURE_RESOURCE_GROUP=your-resource-group

# Azure OpenAI Configuration
AZURE_OPENAI_ENDPOINT=https://foundryuaenorth.cognitiveservices.azure.com/
AZURE_OPENAI_API_KEY=your-azure-openai-api-key

# Azure AI Project Configuration
AZURE_EXISTING_AIPROJECT_ENDPOINT=https://your-ai-project-endpoint.cognitiveservices.azure.com
AZURE_EXISTING_AIPROJECT_RESOURCE_ID=/subscriptions/{subscription-id}/resourceGroups/{resource-group}/providers/Microsoft.CognitiveServices/accounts/{resource-name}/projects/{project-name}

# Azure AI Agent Configuration
AZURE_EXISTING_AGENT_ID=your-primary-agent-id
AZURE_AI_AGENT_NAME=your-agent-name
AZURE_AI_AGENT_DEPLOYMENT_NAME=gpt-4o-mini

# Azure AI Agent Endpoint for SK Orchestration
# This should be the same as AZURE_EXISTING_AIPROJECT_ENDPOINT for most cases
AZURE_AI_AGENT_ENDPOINT=https://your-ai-project-endpoint.cognitiveservices.azure.com

# Azure AI Search Configuration
AZURE_AI_SEARCH_CONNECTION_NAME=your-search-connection-name
AZURE_AI_SEARCH_ENDPOINT=https://your-search-service.search.windows.net
AZURE_AI_SEARCH_INDEX_NAME=your-search-index-name

# Azure AI Embedding Configuration
AZURE_AI_EMBED_DEPLOYMENT_NAME=text-embedding-3-small
AZURE_AI_EMBED_DIMENSIONS=100

# Multi-Agent Orchestration Configuration
# Default agent IDs for multi-agent orchestration (comma-separated)
DEFAULT_ORCHESTRATION_AGENT_IDS=agent-id-1,agent-id-2,agent-id-3
# Prepend message to influence LLM behavior in orchestrated responses
ORCHESTRATION_PREPEND_MESSAGE=You are an expert multi-agent system. Please provide comprehensive, well-structured responses that leverage the unique capabilities of each agent. Consider multiple perspectives and provide detailed analysis with clear reasoning.\n\nUser Question: 

# Web App Authentication (Optional)
WEB_APP_USERNAME=your-username
WEB_APP_PASSWORD=your-password

# Logging Configuration
APP_LOG_FILE=app.log

# Tracing Configuration (Optional)
ENABLE_AZURE_MONITOR_TRACING=false
AZURE_TRACING_GEN_AI_CONTENT_RECORDING_ENABLED=false

# Production Configuration
RUNNING_IN_PRODUCTION=false